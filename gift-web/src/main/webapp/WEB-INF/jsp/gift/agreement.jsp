<script>
    var commodityContractId = getQueryString("commodityContractId");
    var that = this;
    $.ajax({
        url: basePath + "buyCommodity/queryCommodityDetail?time="+new Date(),
        type: "POST",
        data: {'commodityId':commodityContractId},
